<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Vet_Clinic.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Vet_Clinic"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            />
        <ShellContent
            Title="Despre noi"
            ContentTemplate="{DataTemplate local:DespreNoi}"
            />  
        <ShellContent
            Title="Doctori"
            ContentTemplate="{DataTemplate local:DoctorEntryPage}"
            />

        <ShellContent
            Title="Programari"
            ContentTemplate="{DataTemplate local:ViewAppointments}"
            />
    </TabBar>


</Shell>
